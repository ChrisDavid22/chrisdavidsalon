{
  "version": 2,
  "builds": [
    {
      "src": "01-WEBSITE/api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "01-WEBSITE/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/01-WEBSITE/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/01-WEBSITE/$1"
    }
  ]
}